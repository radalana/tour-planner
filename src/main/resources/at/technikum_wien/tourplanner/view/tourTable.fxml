<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="at.technikum_wien.tourplanner.view.TourTableController">
    <children>
    <StackPane AnchorPane.topAnchor="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <Label text="TOUR LIST" underline="true">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
    </StackPane>
    <TableView maxWidth="1.7976931348623157E308" style="-fx-background-radius: 25.0;"
               AnchorPane.leftAnchor="40.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="50.0">
        <columns>
            <TableColumn text="Tour Name"/>
            <TableColumn text="Transport Type"/>
            <TableColumn text="From"/>
            <TableColumn text="To"/>
            <TableColumn text="Distance"/>
            <TableColumn text="Estimated Time"/>
        </columns>
    </TableView>
    </children>
</AnchorPane>
